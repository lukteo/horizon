{
	"name": "client",
	"private": true,
	"version": "0.0.0",
	"type": "module",
	"scripts": {
		"build": "tsc -b && vite build",
		"bump": "bunx npm-check-updates -p bun -ui",
		"check": "biome check --write .",
		"dev": "vite",
		"format": "biome format --write",
		"format:openapi": "openapi-format ../openapi/openapi.yaml -o ../openapi/openapi.yml --sortComponentsProps --no-bundle -c ../openapi/openapi-format-options.json",
		"gen:openapi": "bunx @rtk-query/codegen-openapi ./src/store/api/config.cjs",
		"preview": "vite preview"
	},
	"dependencies": {
		"@hookform/resolvers": "^5.2.2",
		"@reduxjs/toolkit": "^2.11.2",
		"@types/luxon": "^3.7.1",
		"antd": "^6.3.0",
		"i18next": "^25.8.12",
		"i18next-browser-languagedetector": "^8.2.1",
		"luxon": "^3.7.2",
		"react": "^19.2.0",
		"react-dom": "^19.2.0",
		"react-hook-form": "^7.71.1",
		"react-i18next": "^16.5.4",
		"react-redux": "^9.2.0",
		"zod": "^4.3.6"
	},
	"devDependencies": {
		"@biomejs/biome": "2.4.2",
		"@types/node": "^24.10.1",
		"@types/react": "^19.2.7",
		"@types/react-dom": "^19.2.3",
		"@vitejs/plugin-react": "^5.1.1",
		"babel-plugin-react-compiler": "^1.0.0",
		"globals": "^16.5.0",
		"openapi-format": "^1.29.3",
		"typescript": "~5.9.3",
		"vite": "^7.3.1"
	}
}
